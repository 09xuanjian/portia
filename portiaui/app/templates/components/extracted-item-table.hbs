{{#each-in item as |key value|}}
    {{#if (is-array value)}}
        <tr>
            <td>{{key}}</td>
            <td></td>
        </tr>
        <tr>
            <td colspan="2">
                {{#each value key='@index' as |item|}}
                    {{extracted-item-table item=item}}
                {{/each}}
            </td>
        </tr>
    {{else}}
        <tr>
            <td>{{key}}</td>
            <td>
                {{value}}
            </td>
        </tr>
    {{/if}}
{{/each-in}}
